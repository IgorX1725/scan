<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="354.0" prefHeight="354.0" prefWidth="821.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.main.MainController">
   <children>
      <Button fx:id="buttonScan" layoutX="372.0" layoutY="206.0" mnemonicParsing="false" onAction="#onButtonScanAction" prefHeight="57.0" prefWidth="82.0" text="Digitalizar">
         <font>
            <Font name="DejaVu Serif Condensed Bold" size="12.0" />
         </font></Button>
      <Button fx:id="buttonImport" layoutX="366.0" layoutY="94.0" mnemonicParsing="false" onAction="#onButtonImportAction" prefHeight="57.0" prefWidth="93.0" text="Importar">
         <font>
            <Font name="DejaVu Serif Bold" size="12.0" />
         </font></Button>
      <MenuBar fx:id="menuBar" layoutY="2.0" prefHeight="25.0" prefWidth="821.0">
        <menus>
          <Menu mnemonicParsing="false" text="Arquivo">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#onMenuItemListBatch" text="Lotes" />
                  <MenuItem fx:id="exit" mnemonicParsing="false" onAction="#onExitAction" text="Sair" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Editar">
               <items>
                  <MenuItem fx:id="MenuItemsource" mnemonicParsing="false" onAction="#onMenuItemSourceAction" text="Caminho" />
               </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Ajuda">
            <items>
              <MenuItem fx:id="about" mnemonicParsing="false" onAction="#onAboutAction" text="Sobre" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <CheckBox fx:id="checkBoxFrontAndBack" layoutX="473.0" layoutY="206.0" mnemonicParsing="false" selected="" text="Frente e Verso">
         <font>
            <Font size="13.0" />
         </font>
      </CheckBox>
   </children>
</AnchorPane>
